    "probability": {
      "type": "platt_calibrated",
      "calibrator_path": "ml/artifacts/calibrator_platt.pkl"
    },
    "thresholds": {
      "warn_low": 0.1,
      "quarantine": 0.18
    },
    "decision_rules": {
      "normal": "p_cal < warn_low",
      "warning": "warn_low <= p_cal < quarantine",
      "quarantine_recommended": "p_cal >= quarantine"
    },
    "notes": [
      "Thresholds selected for a stable MVP UX. Quarantine threshold uses validated operating point; warn_low introduces a caution zone."
    ]
  },
